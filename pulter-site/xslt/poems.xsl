                        <xsl:attribute name="href">
                          <xsl:value-of select="concat('/poems/ee/', $poemId, '/#ctxs')"/>
                        </xsl:attribute>
                        <xsl:text> </xsl:text>
                      </xsl:element>
                    </xsl:if>
                  </xsl:element>
                </xsl:for-each>
              </ul>
            </div>
          </section>
