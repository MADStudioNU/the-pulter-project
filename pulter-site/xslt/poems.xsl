  <!-- Segments and their renditions -->
  <xsl:template match="seg">
    <xsl:choose>
      <xsl:when test="./@rend">
        <xsl:element name="span">
          <xsl:attribute name="class">
            <xsl:value-of select="./@rend"/>
          </xsl:attribute>
          <xsl:apply-templates/>
        </xsl:element>
      </xsl:when>
      <xsl:otherwise>
        <xsl:apply-templates/>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>

  <!-- Does this poem have "curations" attached? -->
  <xsl:template name="hasCurations">
    <xsl:param name="poemId"/>
    <xsl:choose>
      <xsl:when test="document('')/xsl:stylesheet/pp:poems/pp:poem[@id=$poemId]/pp:curations">true</xsl:when>
      <xsl:otherwise>false</xsl:otherwise>
    </xsl:choose>
  </xsl:template>

  <!-- Curation preview template -->
  <xsl:template name="curations">
    <xsl:param name="poemId"/>
    <xsl:for-each select="document('')/xsl:stylesheet/pp:poems/pp:poem[@id=$poemId]/pp:curations/pp:curation">
      <xsl:variable name="status" select="./@status"/>
      <xsl:if test="$status = 'on'">
        <li class="ctx">
          <xsl:attribute name="data-ctx-hash">
            <xsl:value-of select="./@hash"/>
          </xsl:attribute>
          <xsl:attribute name="data-status">
            <xsl:value-of select="$status"/>
          </xsl:attribute>
          <div class="text">
            <xsl:element name="a">
              <xsl:attribute name="href">
                <xsl:value-of select="concat('./#', ./@hash)"/>
              </xsl:attribute>
              <xsl:attribute name="data-curation-title">
                <xsl:value-of select="./pp:title"/>
              </xsl:attribute>
              <h3 class="lato">
                <xsl:apply-templates select="./pp:title/node()"/>
              </h3>
              <div class="by-line"><span class="by">by</span>
                <span class="who"><xsl:value-of select="./pp:author"/></span></div>
              <xsl:element name="p">
                <xsl:attribute name="class">
                  <xsl:value-of select="'peek'"/>
                </xsl:attribute>
                <xsl:variable name="peekText">
                  <xsl:value-of select="normalize-space(./pp:peek)"/>
                </xsl:variable>
                <xsl:variable name="peekLimit">
                  <xsl:value-of select="150" />
                </xsl:variable>
                <xsl:choose>
                  <xsl:when test="string-length($peekText) > $peekLimit">
                    <xsl:value-of select="concat(substring($peekText, 1, $peekLimit), '…')"/>
                  </xsl:when>
                  <xsl:otherwise>
                    <xsl:value-of select="$peekText"/>
                  </xsl:otherwise>
                </xsl:choose>
              </xsl:element>
            </xsl:element>
          </div>
        </li>
      </xsl:if>
    </xsl:for-each>
  </xsl:template>

  <!-- Poem specific content for meta description HTML tag -->
  <xsl:template name="poemsTopKeywordsChunk">
      <xsl:param name="poemId"/>
      <xsl:param name="numberOfKeywords"/>

      <xsl:variable name="keywords" select="document('')/xsl:stylesheet/pp:poems/pp:poem[@id=$poemId]/pp:keywords/pp:keyword[position() &lt;= $numberOfKeywords]"/>
      <xsl:variable name="count" select="count($keywords)" />

      <xsl:choose>
          <xsl:when test="$count &gt; 1">
              <xsl:for-each select="$keywords">

                  <xsl:if test="position() = 1">
                      <xsl:variable name="keyword" select="text()"/>
                      <xsl:value-of select="concat(' concerning the topics of ', normalize-space($keyword), ', ')"/>
                  </xsl:if>

                  <xsl:if test="position() != last() and position() != 1">
                      <xsl:variable name="keyword" select="text()"/>
                      <xsl:value-of select="concat(normalize-space($keyword), ', ')"/>
                  </xsl:if>

                  <xsl:if test="position() = last()">
                      <xsl:variable name="keyword" select="text()"/>
                      <xsl:value-of select="concat('and ', normalize-space($keyword))"/>
                  </xsl:if>
              </xsl:for-each>
          </xsl:when>

          <xsl:when test="$count = 1">
              <xsl:for-each select="$keywords">
                  <xsl:variable name="keyword" select="text()"/>
                  <xsl:value-of select="concat(' related to ', normalize-space($keyword))"/>
              </xsl:for-each>
          </xsl:when>
          <xsl:otherwise/>
      </xsl:choose>
  </xsl:template>

  <!-- Truncated Folder Name -->
  <xsl:template name="truncateForSEO">
      <xsl:param name="string"/>
      <xsl:param name="length"/>
      <xsl:choose>
          <xsl:when test="string-length($string) > $length">
              <xsl:value-of select="substring($string, 1, $length)"/>
          </xsl:when>
          <xsl:otherwise>
              <xsl:value-of select="$string"/>
          </xsl:otherwise>
      </xsl:choose>
  </xsl:template>

  <!-- String replace template -->
  <xsl:template name="stringReplaceAll">
    <xsl:param name="text" />
    <xsl:param name="replace" />
    <xsl:param name="by" />
    <xsl:choose>
      <xsl:when test="contains($text, $replace)">
        <xsl:value-of select="substring-before($text,$replace)" />
        <xsl:value-of select="$by" />
        <xsl:call-template name="stringReplaceAll">
          <xsl:with-param name="text" select="substring-after($text,$replace)" />
          <xsl:with-param name="replace" select="$replace" />
          <xsl:with-param name="by" select="$by" />
        </xsl:call-template>
      </xsl:when>
      <xsl:otherwise>
        <xsl:value-of select="$text" />
      </xsl:otherwise>
    </xsl:choose>
  </xsl:template>

  <!-- Keyword string formatter -->
  <xsl:template name="keywordStringFormatter">
    <xsl:param name="input"/>

    <xsl:variable name="keywordValue" select="$input"/>
    <xsl:variable name="keywordNLC" select="translate(normalize-space($keywordValue), $ucAlphabet, $lcAlphabet)"/>
    <xsl:variable name="keywordNRP" select="translate(normalize-space($keywordNLC), ')', '')"/>
    <xsl:variable name="keywordNLP" select="translate(normalize-space($keywordNRP), '(', '')"/>
    <xsl:variable name="keywordOut" select="translate(normalize-space($keywordNLP), ' ', '-')"/>

    <xsl:value-of select="$keywordOut"/>
  </xsl:template>
</xsl:stylesheet>
