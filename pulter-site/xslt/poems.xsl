                                <xsl:if test="$editionType = 'elemental'">
                                  <xsl:value-of select="'ee'"/>
                                </xsl:if>
                                <xsl:if test="$editionType = 'amplified'">
                                  <xsl:value-of select="'ae'"/>
                                </xsl:if>
                              </xsl:variable>
                              <xsl:variable name="editionIdUrlHash">
                                <xsl:if test="$editionType = 'amplified' and @id">
                                  <xsl:value-of select="concat('#', @id)"/>
                                </xsl:if>
                              </xsl:variable>
                              <xsl:element name="li">
                                <xsl:attribute name="class">
                                  <xsl:value-of select="'edition'"/>                                          </xsl:attribute>
                                <xsl:if test="$editionTypeUrlChunk">
                                  <xsl:attribute name="class">
                                    <xsl:value-of select="concat('edition ', $editionTypeUrlChunk)"/>
                                  </xsl:attribute>
                                </xsl:if>
                                <xsl:element name="a">
                                  <xsl:attribute name="title">
                                    <xsl:value-of select="concat('Open ', $editionType, ' edition')"/>
                                  </xsl:attribute>
                                  <xsl:attribute name="href">
                                    <xsl:value-of select="concat('/poems/', $editionTypeUrlChunk, '/', $poemId, $editionIdUrlHash)"/>
                                  </xsl:attribute>
                                  <xsl:attribute name="class">
                                    <xsl:value-of select="'edition-button'"/>
                                  </xsl:attribute>
                                  <xsl:element name="div">
                                    <xsl:attribute name="class">
                                      <xsl:value-of select="'edition-code'"/>
                                    </xsl:attribute>
                                    <xsl:choose>
                                      <xsl:when test="$editionLabel">
                                        <xsl:value-of select="$editionLabel"/>
                                      </xsl:when>
                                      <xsl:otherwise>
                                        <xsl:value-of select="concat($editionType, ' edition')"/>
                                      </xsl:otherwise>
                                    </xsl:choose>
                                  </xsl:element>
                                  <xsl:element name="div">
                                    <xsl:attribute name="class">
                                      <xsl:value-of select="'by-line'"/>
                                    </xsl:attribute>
                                    <xsl:element name="span">
                                      <xsl:attribute name="class">
                                        <xsl:value-of select="'by'"/>
                                      </xsl:attribute>
                                      <xsl:text>by </xsl:text>
                                    </xsl:element>
                                    <xsl:for-each select="./pp:author">
                                      <xsl:choose>
                                        <xsl:when test="not(position() = last())">
                                          <xsl:element name="span">
                                            <xsl:attribute name="class">
                                              <xsl:value-of select="'who'"/>
                                            </xsl:attribute>
                                            <xsl:value-of select="."/>
                                          </xsl:element>
                                          <xsl:text> </xsl:text>
                                          <xsl:element name="span">
                                            <xsl:attribute name="class">
                                              <xsl:value-of select="'by'"/>
                                            </xsl:attribute>
                                            <xsl:text>and</xsl:text>
                                          </xsl:element>
                                          <xsl:text> </xsl:text>
                                        </xsl:when>
                                        <xsl:otherwise>
                                          <xsl:element name="span">
                                            <xsl:attribute name="class">
                                              <xsl:value-of select="'who'"/>
                                            </xsl:attribute>
                                            <xsl:value-of select="."/>
                                          </xsl:element>
                                        </xsl:otherwise>
                                      </xsl:choose>
                                    </xsl:for-each>
                                  </xsl:element>
                                </xsl:element>
                              </xsl:element>
                            </xsl:for-each>
                          </xsl:element>
                        </xsl:when>
                    </xsl:choose>
                    </div>
                    <xsl:if test="$isPublished and $hasCurations">
                      <xsl:element name="a">
                        <xsl:attribute name="class">
                          <xsl:value-of select="'js-link to-curation'"/>
                        </xsl:attribute>
                        <xsl:attribute name="title">
                          <xsl:value-of select="'Jump to curations'"/>
